    "import numpy as np\n",
    "import jax.numpy as jnp\n",
    "x = jnp.array(np.random.randint(0, 10, size=(4,4)), dtype=jnp.int16)\n",
    "bits = jnp.unpackbits(x.view(jnp.uint8)).reshape(x.size, 16)[:, 1:]\n",
    "print(bits)"
